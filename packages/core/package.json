{
  "name": "@sff-ui/core",
  "version": "0.1.4",
  "description": "SFF - Small Frontend Framework",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "frontend",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kotoshii/small-frontend-framework.git"
  },
  "bugs": {
    "url": "https://github.com/kotoshii/small-frontend-framework/issues"
  },
  "author": "kotoshii",
  "license": "GPL-3.0",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc & tsc-alias",
    "release": "commit-and-tag-version -t @sff-ui/core/v",
    "release:publish": "yarn build && npm publish"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "commit-and-tag-version": "^12.4.4",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "commit-and-tag-version": {
    "skip": {
      "changelog": true
    },
    "releaseCommitMessageFormat": "chore(release/core): {{currentTag}}"
  }
}
