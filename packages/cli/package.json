{
  "name": "@sff-ui/cli",
  "version": "0.1.2",
  "description": "SFF CLI - A CLI tool for Small Frontend Framework",
  "bin": {
    "sff-ui": "dist/bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "frontend",
    "framework",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kotoshii/small-frontend-framework.git"
  },
  "bugs": {
    "url": "https://github.com/kotoshii/small-frontend-framework/issues"
  },
  "author": "kotoshii",
  "license": "GPL-3.0",
  "scripts": {
    "copy-files": "ncp templates dist/templates",
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc & tsc-alias",
    "release": "commit-and-tag-version -t @sff-ui/cli/v",
    "release:publish": "yarn build && npm publish"
  },
  "devDependencies": {
    "@types/ncp": "^2.0.8",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "commit-and-tag-version": "^12.4.4",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "ncp": "^2.0.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "commit-and-tag-version": {
    "skip": {
      "changelog": true
    }
  }
}
